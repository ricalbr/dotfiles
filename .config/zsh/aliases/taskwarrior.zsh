# TASKWARRIOR ALIASES
alias task='task 2>/dev/null'

alias tt='clear;task next'
alias tw='clear;task +work next'
alias ta='task add'
alias tm='task mod'
alias tn='task mod +next'
alias td='task done -next'

alias tapl='task add pri:L '
alias taph='task add pri:H '
alias tatp='task add +personal'
alias tatw='task add +work'
